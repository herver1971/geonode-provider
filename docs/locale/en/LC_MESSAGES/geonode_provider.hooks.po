#: geonode_provider.hooks.geonode.GeoNodeHook:4 of
msgid ""
"import_layer: runs the \"importlayers\" command in the GeoNode container "
"to upload the datasets contents in a specific directory/folder to "
"GeoServer."
msgstr ""

#: geonode_provider.hooks.geonode.GeoNodeHook:5 of
msgid "publish_layer: publishes a layer uploaded to the database in GeoServer."
msgstr ""

#: geonode_provider.hooks.geonode.GeoNodeHook:6 of
msgid ""
"update_layer: runs the \"updatelayers\" command in the GeoNode container "
"so that the layer already uploaded, published and recognized by "
"GeoServer, is now visible in GeoNode."
msgstr ""

#: geonode_provider.hooks.geonode.GeoNodeHook:7 of
msgid ""
"upload_dataset: uploads a layer in *.zip*, \".shp\", \"dbf\", \"shx\", "
"\"prj\" or \".tif\" format to GeoNode via API v2."
msgstr ""

#: geonode_provider.hooks.geonode.GeoNodeHook:8 of
msgid "upload_style: uploads a style in \".sld\" format to GeoServer."
msgstr ""

#: geonode_provider.hooks.geonode.GeoNodeHook:9 of
msgid "update_style: update an existing style."
msgstr ""

#: geonode_provider.hooks.geonode.GeoNodeHook:10 of
msgid "update_layer_style: assigns an already uploaded style to a layer."
msgstr ""

#: geonode_provider.hooks.geonode.GeoNodeHook:11 of
msgid "upload_map: creates a map in GeoNode via API v2 from a set of datasets."
msgstr ""

#: geonode_provider.hooks.geonode.GeoNodeHook:12 of
msgid "upload_document: uploads a document to GeoNode via API v2."
msgstr ""

#: geonode_provider.hooks.geonode.GeoNodeHook:13 of
msgid ""
"upload_metadata: uploads/sets metadata for a given type of resource (for "
"the moment title and supplementary information)."
msgstr ""

#: geonode_provider.hooks.geonode.GeoNodeHook:14 of
msgid "set_permiss: sets permissions for a given resource."
msgstr ""

#: geonode_provider.hooks.geonode.GeoNodeHook:15 of
msgid ""
"get_execution_information: gets information/metadata about a performed "
"execution."
msgstr ""

